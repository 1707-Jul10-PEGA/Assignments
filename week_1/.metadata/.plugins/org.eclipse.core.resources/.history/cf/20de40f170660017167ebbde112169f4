package com.md.q2.driver;

public class Driver {

	public static void main(String[] args) {
		fib(0,5);
	}
	
	public static int fib(int n, int counter) {
		//base case
		if(counter == 0) {
			return n;
		}
		
		//recursive case
		else {
			counter++;
			return fib(n+1) + fib(n+2);
		}
			
		
		
	}
	
	
}
