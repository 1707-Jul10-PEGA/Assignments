package com.md.q2.driver;

public class Driver {

	public static void main(String[] args) {
		fib(5);
	}
	
	public static void fib(int n) {
		//base case
		if(n == 0) {
			return ;
		}
		
		//recursive case
		else {
			System.out.println("Fib of " + (fib(n-2) + fib(n-1)));
			
		}
			
		
		
	}
	
	
}
